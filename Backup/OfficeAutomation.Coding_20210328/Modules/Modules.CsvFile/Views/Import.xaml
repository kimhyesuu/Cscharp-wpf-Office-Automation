<UserControl x:Class="Modules.CsvFile.Views.Import"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         	 xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
 
				 xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
		<Button Margin="1" Width="45" Height="40"  
					  Background="DeepSkyBlue" BorderBrush="{x:Null}"
					  Command="{Binding Path=OpenFileDialogCommand}">
			<materialDesign:PackIcon Kind="FileOutline" Width="15" Height="15" Foreground="Black" HorizontalAlignment="Center"/>
		</Button>
	</Grid>
</UserControl>
