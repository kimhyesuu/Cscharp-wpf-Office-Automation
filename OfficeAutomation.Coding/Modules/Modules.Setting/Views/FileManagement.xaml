<UserControl x:Class="Modules.Setting.Views.FileManagement"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
				 xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
 
				 xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"	>
				
		<StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center"	>
		<Button Margin="5,5" Width="100" Height="35" Content="Import"		 FontWeight="Bold" 
					  Command="{Binding Path=ImportCommand}" Background="#81695B" BorderBrush="#1C1714" />

		<Button Margin="0,5" Width="100" Height="35" Content="Export"		 FontWeight="Bold" 
					  Command="{Binding Path=ExportCommand}"	 Background="#81695B" BorderBrush="#1C1714"  />
			<Button Margin="5,5" Width="100" Height="35" Content="Folder" FontWeight="Bold" 
					  Command="{Binding Path=OpenFolderCommand}" Background="#81695B" BorderBrush="#1C1714" />
		</StackPanel>
</UserControl>
